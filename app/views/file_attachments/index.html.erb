<h1>Listing file_attachments</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Desc</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @file_attachments.each do |file_attachment| %>
  <tr>
    <td><%= file_attachment.title %></td>
    <td><%= file_attachment.desc %></td>
    <td><%= link_to 'Show', file_attachment %></td>
    <td><%= link_to 'Edit', edit_file_attachment_path(file_attachment) %></td>
    <td><%= link_to 'Destroy', file_attachment, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New File attachment', new_file_attachment_path %>
